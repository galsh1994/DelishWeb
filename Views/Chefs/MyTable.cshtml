@model IEnumerable<delishapplication.Models.Chef>

@{
    ViewData["Title"] = "MyTable";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>MyTable</h2>

<ul>
    @foreach (var chef in ViewBag.ourList)
    {
        <li>@chef.Popularity</li>

    }
    
</ul>

